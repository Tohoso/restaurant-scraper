{
  "project": {
    "name": "restaurant-scraper",
    "description": "Restaurant data scraper for Tabelog and HotPepper",
    "version": "1.0.0",
    "type": "python"
  },
  "environment": {
    "python_version": "3.8+",
    "dependencies": [
      "requests>=2.28.0",
      "beautifulsoup4>=4.11.0",
      "pandas>=1.5.0",
      "openpyxl>=3.0.0",
      "lxml>=4.9.0"
    ]
  },
  "scripts": {
    "setup": "python3 setup.py",
    "run": "python3 restaurant_scraper_app.py",
    "interactive": "python3 restaurant_scraper_app.py -i",
    "test": "python3 -m py_compile *.py"
  },
  "files": {
    "main": "restaurant_scraper_app.py",
    "modules": [
      "hotpepper_api_client.py",
      "tabelog_scraper_improved.py",
      "restaurant_data_integrator.py"
    ],
    "config": [
      "requirements.txt",
      "setup.py"
    ],
    "docs": [
      "README.md"
    ]
  },
  "output": {
    "directory": "./",
    "format": "xlsx",
    "log": "restaurant_scraper.log"
  }
}